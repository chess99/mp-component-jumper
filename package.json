{
  "name": "mp-component-jumper",
  "displayName": "MP Component Jumper",
  "description": "",
  "version": "0.1.3",
  "engines": {
    "vscode": "^1.19.0"
  },
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "rm -rf ./dist && tsc -p ./",
    "watch": "rm -rf ./dist && tsc -watch -p ./",
    "lint": "eslint ./src --ext ts"
  },
  "devDependencies": {
    "@types/node": "^18.17.17",
    "@types/vscode": "^1.19.0",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "publisher": "chess99",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chess99/mp-component-jumper.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:wxml",
    "onLanguage:json"
  ],
  "contributes": {
    "languages": [
      {
        "id": "wxml",
        "extensions": [
          ".wxml"
        ],
        "aliases": [
          "WXML"
        ]
      }
    ]
  }
}
